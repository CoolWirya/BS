      <Query>
        <DataSourceName>erpSahamConnectionString</DataSourceName>
        <CommandText>SELECT A.pcode, A.sumpay, A.Az, A.Ela, A.Managementsharescount, A.lastpaydate, A.coursecode , A.doccode, A.sheetno, sahamcourse.title as coursetitle, sahamcourse.finyear  , sahamdocument.docdesc, cashes.name as paylocname  , P.FirstName, P.LastName, P.FatherName, P.ShSh, P.shMelli 
             FROM (SELECT sahampayment.pcode, Sum(paycost)as sumpay ,MIN(Managementsharesno)  Az, MAX(Managementsharesno) Ela
            , max(Managementsharesno) - min(Managementsharesno) +1  as Managementsharescount , Max(paydate) 
	as lastpaydate, sahampayment.sheetno, sahampayment.coursecode ,sahampayment.doccode  from sahampayment group by pcode , sheetno ,sahampayment.doccode , sahampayment.coursecode )  as A	inner join sahamcourse on A.coursecode = sahamcourse.code	
inner join sahamdocument on doccode = sahamdocument.code left outer join erp.subdefine cashes on cashes.code= sahamdocument.payloc 	inner join erp.vperson P on P.pcode = A.pcode WHERE A.pcode&gt;0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
